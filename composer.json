	"require": {
		"php": ">=7.2.0",
		"ext-json": "*",
		"ext-sodium": "*",
		"slim/slim": "^4.5",
		"vlucas/phpdotenv": "^5.1",
		"slim/psr7": "^1.2",
		"illuminate/database": "^8.1",
		"nesbot/carbon": "^2.39",
		"illuminate/filesystem": "^8.1",
		"illuminate/pagination": "^8.1",
		"kitetail/zttp": "^0.6.0",
		"hassankhan/config": "^2.1",
		"dflydev/fig-cookies": "^2.0",
		"slim/flash": "^0.4.0",
		"slim/twig-view": "^3.1",
		"kanellov/slim-twig-flash": "^0.2.0",
		"php-di/php-di": "^6.2",
		"php-di/slim-bridge": "^3.0",
		"middlewares/trailing-slash": "^2.0",
		"monolog/monolog": "^2.1",
		"oscarotero/env": "^2.1",
		"doctrine/annotations": "^1.10",
		"zeuxisoo/slim-whoops": "^0.7.2",
		"twig/twig": "^v3.0",
		"paragonie/random_compat": "^2.0",
		"paragonie/random-lib": "^2.0",
		"bryanjhv/slim-session": "^4.1"
    },
	"require-dev": {
		"phpunit/phpunit": "^9.3",
