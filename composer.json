{
    "name": "voltra/slim-vue-app",
    "description": "A good to go project setup for Slim VueJS Twig applications",
    "type": "library",
    "authors": [
        {
            "name": "Voltra",
            "email": "ludwig.guerin.98@gmx.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "slim/slim": "^3.9",
        "slim/twig-view": "^2.3",
        "hassankhan/config": "^0.11.2",
        "illuminate/database": "^5.5",
        "slim/flash": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "php-mock/php-mock": "^2.0",
        "php-mock/php-mock-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "dev",
            "App\\Tests\\": "Tests"
        }
    }
}
