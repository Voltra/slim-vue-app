    "compary": "^0.1.0",
    "store": "^2.0.12",
    "db.js": "^0.15.0",
    "spinner-lord": "^0.2.6",
    "@voltra/json": "^1.2.0",
    "vanilla_flash": "^2.0.0"
  }
