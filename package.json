{
  "name": "slim-vue-app",
  "version": "1.0.0",
  "description": "An application that uses Slim and Vue (with Twig)",
  "main": "index.js",
  "scripts": {
    "setup": "npm run setupCLI && npm run setupJS && npm run setupPHP && npm run setupRoutes",
    "setupRoutes": "source aliases.sh && dumpRouteLoader",
    "setupCLI": "go get -u github.com/turnitin/dbmate",
    "setupJS": "npm i",
    "setupPHP": "composer update && composer dump-autoload",
    "build": "webpack",
    "watch": "webpack --watch",
    "testPHP": "punit",
    "testJS": "npm run unitJS && npm run e2e",
    "unitJS": "jest --colors",
    "e2e": "nightwatch",
    "test": "npm run testPHP && npm run testJS"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "file-loader": "^3.0.1",
    "geckodriver": "^1.14.1",
    "imagemin": "^6.1.0",
    "img-loader": "^3.0.1",
    "jest": "^23.6.0",
    "nightwatch": "^1.0.18",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.5.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "dependencies": {
    "vue": "^2.5.22",
    "vuex": "^3.0.1",
