{
  "name": "slim-vue-app",
  "version": "1.0.0",
  "description": "An application that uses Slim and Vue (with Twig)",
  "main": "index.js",
  "scripts": {
    "setup": "npm run setupJS && npm run setupPHP",
    "setupJS": "npm install",
    "setupPHP": "composer update && composer dump-autoload",
    "build": "webpack",
    "watch": "webpack --watch",
    "setToDev": "cross-env NODE_ENV=dev",
    "setToProd": "cross-env NODE_ENV=production",
    "dev-build": "npm run setToDev && npm run build",
    "dev-watch": "npm run setToDev && npm run watch",
    "prod-build": "npm run setToProd && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "img-loader": "^2.0.0",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.9",
    "webpack": "^3.10.0",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "vue": "^2.5.17"
  }
}
